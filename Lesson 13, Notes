izohli_lugat = {'int': 'butun son qilib keltirib olish uchun ishlatiladi',
                'float': "sonlarni o'nli ko'rinishga o'tkazib olish uchun ishlatiladi", 
               'str': "matnlar yoki sonlarni tekst ko'rinishga o'tkazish uchun ishlatiladi",
                'if': "agar degan ma'noni bildirib, tarmoqlanish uchun foydalaniladi",
                'else':"boshqa holatlarda degan ma'noni bildirib, barcha boshqa holatlarda ishaltiladi", }
so_z = input("Xush kelibsiz. Python dasturlash tiliga aloqador bo'lgan biror-bir so'zni kiriting >>>")
print(izohli_lugat.get(so_z, "Bunday atama ushbu lug'atda mavjud emas"))

izohli_lugat = {'int': 'butun son qilib keltirib olish uchun ishlatiladi',
                'float': "sonlarni o'nli ko'rinishga o'tkazib olish uchun ishlatiladi", 
               'str': "matnlar yoki sonlarni tekst ko'rinishga o'tkazish uchun ishlatiladi",
                'if': "agar degan ma'noni bildirib, tarmoqlanish uchun foydalaniladi",
                'else':"boshqa holatlarda degan ma'noni bildirib, barcha boshqa holatlarda ishaltiladi", }
so_z = input("Xush kelibsiz. Python dasturlash tiliga aloqador bo'lgan biror-bir so'zni kiriting >>>")
if so_z in izohli_lugat:
    print(f"{izohli_lugat[so_z]}")
else:
    print("Ushbu so'z bizning lug'atimizda mavjud emas")
